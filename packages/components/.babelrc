{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react",
  ],
  "plugins": [
    ["babel-plugin-styled-components", {
      "displayName": false,
      "pure": true
    }],
    ["babel-plugin-module-resolver", {
      "root": ["./src"],
      "alias": {
        "@colors": "./src/constants/colors",
        "@fonts": "./src/constants/fonts"
      }
    }]
  ]
}
